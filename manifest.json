{ 
    "manifest_version": 2,
    "author": "Ivan Phelps",
    "name": "Duck It",
    "content_scripts": [{
        "js": ["./src/javascripts/main.js"],
        "matches": ["http://*/*", "https://*/*"]
    }],
    "description": "Replaces all photos with the same photo of a duck",
    "icons": {
        "48": "./src/images/duck-icon.png",
        "96": "./src/images/duck-icon@2x.png"
    },
    "version": "0.1",
    "web_accessible_resources" : [ 
        "./src/images/duck-icon.png",
        "./src/images/duck-icon@2x.png",
        "./src/images/duck-large.png",
        "./src/javascripts/main.js"
    ] 
}