{ 
    "manifest_version": 2,
    "author": "Ivan Phelps",
    "name": "Duck It",
    "content_scripts": [{
        "js": ["./src/duckIt.js"],
        "matches": ["http://*/*", "https://*/*"]
    }],
    "description": "Replaces all photos with the same photo of a duck",
    "icons": {
        "48": "./src/images/duck-icon.png",
        "96": "./src/images/duck-icon@2x.png"
    },
    "version": "0.1",
    "web_accessible_resources" : [ 
        "https://raw.githubusercontent.com/ivannio/Duck_It/master/src/images/duck-icon.png",
        "https://raw.githubusercontent.com/ivannio/Duck_It/master/src/images/duck-icon%402x.png",
        "https://raw.githubusercontent.com/ivannio/Duck_It/master/src/images/duck-large.png" 
    ] 
}